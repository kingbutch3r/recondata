                if ($("#"+containerID).isInViewportAnimation() || isSafari) {
                  loadAnimationOnce();
                }
              });

              window.onload = function (event) {
                if ($("#"+containerID).isInViewportAnimation() || isSafari) {
                  loadAnimationOnce();
                }
              };

              $(window).scroll(function() {
                  // Run animation only if in viewport
                  if ($("#"+containerID).isInViewportAnimation() ) {
                    loadAnimationOnce();
                    if (autoplayValue) {
