    (function($) {
      $(document).ready(function() {

        $.fn.isInViewportAnimation = function () {
          var elementTop = $(this).offset().top,
            elementBottom = elementTop + $(this).outerHeight(),
            viewportTop = $(window).scrollTop(),
            viewportBottom = viewportTop + $(window).height();
          return elementBottom > viewportTop && elementTop < viewportBottom;
        };

          (function() {
              let stylesheetPath = "https://www.payoneer.com/wp-content/themes/bridge-child",
              jsonName = "mobile-animation",
              containerID = "mobile-animation-animation",
              autoplayValue = true,
              loopValue = true,
              containerAnimation = document.getElementById(containerID),
              animationLoaded,
              animation;
              let isSafari = /^((?!chrome|android).)*safari/i.test(navigator.userAgent); //Safari bug - do the check if Safari

              function loadAnimationOnce() {
                if (!animationLoaded) {
                  animationLoaded = true;
                    animation = lottie.loadAnimation({
                        container: containerAnimation,
                        renderer: 'svg',
                        autoplay: autoplayValue,
                        loop: loopValue,
                        prerender: true,
                        path: stylesheetPath + '/assets/svg-animations/' + jsonName + '.json'
                    });
                    animation.addEventListener('DOMLoaded', function(e) {
                      //needed to give fade effect
