    .primary-text-background { background-color: #2A3760; }
    .primary-text-border { border-color: #2A3760; }
    .secondary-text-color { color: #BAB9B9; }
    .secondary-text-background { background-color: #BAB9B9; }
    .secondary-text-border { border-color: #BAB9B9; }

  .btn-primary {
    background-color: #8380FF !important;
    color: #FFFFFF !important;
    border-color: #8380FF !important;
  }

  .btn-outline-primary {
    border-color: #8380FF !important;
    color: #8380FF !important;
    outline: unset !important;
  }

  .btn-outline-primary:hover, .btn-outline-primary:active, .btn-outline-primary:focus {
    border-color: #8380FF !important;
    background-color: #8380FF !important;
    color: #FFFFFF !important;
  }

  .custom-control-input:not(:disabled) ~ .custom-control-label {
    cursor: pointer;
  }

  .custom-control-input:checked ~ .custom-control-label:before {
    border-color: #8380FF !important;
    background-color: #8380FF !important;
  }

  .custom-control-input:checked:disabled ~ .custom-control-label:before {
    border-color: #D4D5D8 !important;
    background-color: #D4D5D8 !important;
  }

  .ln-end-user-layout .form-control::placeholder {
    color:rgba(42, 55, 96, 0.35)
  }

  .work-item-outer-container:hover {
    color:#2A3760;
  }

  input#search {
    background-color: #FFFFFF;
    border-bottom-color: #FFFFFF;
  }

  input#search:focus {
    border-bottom-color: #8380FF;
  }

  .navbar-brand, .navbar-brand:hover {
    color: #FFFFFF !important;
  }

  .brand-circle {
    color: #8380FF;
    background-color: #FFFFFF;
  }

  .views.navbar {
    border-top: 1px solid #F3F6F8;
  }

  .footie p, .footie a, .footie a:hover {
    color: #BAB9B9 !important;
  }

