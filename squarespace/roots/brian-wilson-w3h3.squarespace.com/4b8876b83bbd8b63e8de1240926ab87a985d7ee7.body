                            duration: 2,
                            left: 0
                        });

                    rightChair.setStyles({
                        'right': '-' + offset + 'px'
                    })
                        .transition({
                            duration: 2,
                            right: 0
                        }, hideAncillary);
                };

                showAncillary();

            },

