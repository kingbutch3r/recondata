                    });
                    /*whiteChair.transition({
                      duration: 2.5,
                      delay: 4,
                      right: '-' + (rightPx + 10) + 'px'
                    });*/

                    text.transition({
                        duration: 2.5,
                        delay: 4,
                        left: '25%'
                    }, showArrangementThree)
