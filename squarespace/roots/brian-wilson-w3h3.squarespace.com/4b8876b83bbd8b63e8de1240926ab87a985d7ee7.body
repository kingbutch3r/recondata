                    that = this;

                function hideArrangementThree() {
                    var leftPx = backTable.get('width');

                    backTable.transition({
                        duration: 2,
                        delay: 4,
                        left: '-' + (leftPx + 10) + 'px'
                    });
                    backChair.transition({
                        duration: 2,
                        delay: 4,
                        left: '-' + (leftPx + 50) + 'px'
