                        if (titleWrapperNode && (titleDescImageNode = titleWrapperNode.one('> div.title-desc-image'))
                            && (inner = titleDescImageNode.next('div.title-desc-inner'))) {
                            inner.hide();
                        }
                    }

                    hello.remove();
                    imageContainer.appendChild(clone);
                }
            },

            getImagePosition: function (node) {
