                        duration: 2,
                        delay: 4,
                        left: '-' + (leftPx + 300) + 'px'
                    });

                    text.transition({
                        duration: 2,
                        delay: 4,
                        left: '25%'
                    }, showAncillary);
                }

                function showArrangementThree() {
                    var threeImages = new Y.NodeList([backTable, backChair, frontTable, frontChair]),
                        leftPx = backTable.get('width');
                    backTable.setStyle('left', '-' + (leftPx + 20) + 'px');
                    backChair.setStyle('left', '-' + (leftPx + 100) + 'px');
                    frontTable.setStyle('left', '-' + (leftPx + 500) + 'px');
                    frontChair.setStyle('left', '-' + (leftPx + 800) + 'px');

                    threeImages.transition({
                        duration: 2,
                        left: that.getImagePosition(backTable) + 'px'
                    });

                    text.transition({
                        duration: 2,
                        left: '50%'
                    }, hideArrangementThree);

                };

                function hideArrangementTwo() {
