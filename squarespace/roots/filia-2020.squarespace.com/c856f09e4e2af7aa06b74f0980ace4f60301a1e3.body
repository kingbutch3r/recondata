        function animatedCounter() {
          var elementsToWatch = document.querySelectorAll('.animated-counter');
          var config = {
            rootMargin: '0px',
            threshold: 0
          };

          var observer = new IntersectionObserver(function (entries, self) {
            entries.forEach(entry => {
              if (entry.isIntersecting) {
                self.unobserve(entry.target);
                console.log('Block' + entry.target + ' is in the viewport!');
                entry.target.classList.add('in-viewport');
                let animatedCounter = $(entry.target).find('.animated-counter--count');
                animatedCounter.prop('Counter',0).animate({
                  Counter: animatedCounter.text()
                }, {
                  duration: 3000,
                  easing: 'swing',
                  step: function (now) {
                    animatedCounter.text(Math.ceil(now));
                  }
                });
              }              
            });
          }, config);
          elementsToWatch.forEach(ele => { observer.observe(ele); });
        }
        function ticketDate() {
          
          //HTML example
          //<div id="eventCountdown" data-countdown-date="Oct 16, 2020 09:00:00"></div>

          if($('#eventCountdown[data-countdown-start-date]').length) {
            let countdownContainer = $('#eventCountdown').eq(0),
                eventStartDate = countdownContainer.attr('data-countdown-start-date'),
                countdownStartDate = new Date(eventStartDate).getTime(),
                eventEndDate = countdownContainer.attr('data-countdown-end-date'),
                countdownEndDate = new Date(eventEndDate).getTime();

            //console.log('eventStartDate',eventStartDate);
            //console.log('countdownStartDate',countdownStartDate);

            // Update the count down every 1 second
            var x = setInterval(function() {

              // Get the date and time every 1000ms
              var now = new Date().getTime();

              // Find the distance between now and the count down date
              var distance = countdownStartDate - now;

              // Time calculations for days, hours, minutes and seconds
              var days = Math.floor(distance / (1000 * 60 * 60 * 24));
              var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
              var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
              var seconds = Math.floor((distance % (1000 * 60)) / 1000);

              // If the current date/time is before the start date/time of the event, display the countdown
              if (distance > 0) {
                // Display the result in the element with id="eventCountdown"
                countdownContainer.html('<div class="countdown-inner"><div class="countdown-item"><div class="countdown-item-inner"><div>'+days+'</div><div>days</div></div></div><div class="countdown-item"><div class="countdown-item-inner"><div>'+hours+'</div><div>hours</div></div></div><div class="countdown-item"><div class="countdown-item-inner"><div>'+minutes+'</div><div>minutes</div></div></div><div class="countdown-item"><div class="countdown-item-inner"><div>'+seconds+'</div><div>seconds</div></div></div></div>');
                //Show event live text and countdown
                $('#eventIsCurrentlyLive').show();
              }
              // If the current date/time is between the start date/time of the event and the end date/time of the event, display the id="eventLive" text
              else if (countdownEndDate - now > 0) {
                //Stop timer
                clearInterval(x);
                //Hide countdown
                $('#eventCountdown').eq(0).hide();
                //Show event live text
                $('#eventIsCurrentlyLive').show();
              }
              else {
                //Hide event live text
                $('#eventIsCurrentlyLive').hide();
                //Show event finished text
                $('#eventIsFinished').show();
              }
            }, 1000);
          }
        }
