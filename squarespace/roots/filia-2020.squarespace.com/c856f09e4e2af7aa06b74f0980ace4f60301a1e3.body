        function accordion(){
          
          function isEven(n){return n%2==0}
          function isOdd(n){return Math.abs(n%2)==1}
          
          //Make the first accordion item active on page start
          //$('.sqs-layout .sqs-block-markdown').eq(0).addClass('first-accordion-item activedrop');

          //Identify which style of accordion to display based on the heading type in the Markdown block
          //Accordion style 1 when Markdown contains a Heading 2
          $('.sqs-block-markdown')
            .find('h2:first-child')
            .closest('.sqs-block-markdown')
            .addClass('accordion-item accordion-style-1')
            .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
            .addClass('accordion-item accordion-style-1');
          //Accordion style 2 when Markdown contains a Heading 3
          $('.sqs-block-markdown')
            .find('h3:first-child,p:first-child')
            .closest('.sqs-block-markdown')
            .addClass('accordion-item accordion-style-2')
            .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
            .addClass('accordion-item accordion-style-2');

          $('.sqs-layout .sqs-block-markdown.accordion-style-1').each(function(index){
            if (isEven(index)) {
              $(this)
              .addClass('even accordion-item-'+index)
              .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
              .addClass('even accordion-item-'+index)
            }
            if (isOdd(index)) {
              $(this)
              .addClass('odd accordion-item-'+index)
              .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
              .addClass('odd accordion-item-'+index)
            }           
          });
          
          //Collapse all but the first accordion on page start
          //$('.sqs-block-markdown:not(.first-accordion-item)').nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer').slideUp(0);
          
          //Collapse all accordions on page start
          $('.sqs-block-markdown')
            .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
            .slideUp(0);
          
          $('.sqs-block-markdown').unbind().click(function(){
            if ($(this).hasClass('accordion-style-1') || $(this).hasClass('accordion-style-2')) {
              //Collapse all other accordion items
              $('.sqs-block-markdown.accordion-style-1,.sqs-block-markdown.accordion-style-2')
                .not(this)
                .removeClass('activedrop')
                .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
                .slideUp(750)
                .removeClass('reveal',750)
            }
            //Collapse or open this accordion item
            $(this)
            .toggleClass('activedrop')
            .nextUntil('.sqs-block-markdown, .sqs-block-code, .sqs-block-spacer')
            .slideToggle(750)
            .toggleClass('reveal',750)
          });
