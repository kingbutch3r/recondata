                allItems = {'items':[]};

                //Start downloading the items
                loop(null,null)
              }

            });

            $(document).on('click','[data-filter]',function() {

              if(!downloading) {
              
                var $this = $(this);
                var text = $this.text();
                var parameter = $this.attr('data-filter');
