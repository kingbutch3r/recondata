            images[i].classList.add('loaded');
            images[i].parentNode.classList.add('loaded');
            //Remove the background colour from items after image is loaded
            let bgColourToRemove = images[i].parentNode.parentNode;
            if(bgColourToRemove.className.match(new RegExp('(\\s|^)remove-background-colour-after-image-loaded(\\s|$)'))){
              bgColourToRemove.removeAttribute('style');
            }
          }
        }
        function calculateAllImageRatios(ele) {
          if($(ele).length){
            let imageContainer = document.querySelectorAll(ele);
            for (let i = 0; i < imageContainer.length; i++) {
              //Get image dimensions
              let imageWidth = parseInt(imageContainer[i].dataset.imageWidth,10);
              let imageHeight = parseInt(imageContainer[i].dataset.imageHeight,10);
              let ratio = (imageHeight/imageWidth)*100;
              let paddingBottom;
              if (ratio < 100) {
                paddingBottom = "100";
              }
              else {
                paddingBottom = "" + (imageHeight/imageWidth)*100;
              }
              //Set size (as a ratio) of image container
              imageContainer[i].style.paddingBottom = paddingBottom + "%";
              if(imageWidth < imageHeight) {
                imageContainer[i].classList.add('portrait');
                imageContainer[i].parentNode.classList.add('portrait');
              }
              else if(imageWidth > imageHeight) {
                imageContainer[i].classList.add('landscape');
                imageContainer[i].parentNode.classList.add('landscape');
              }
              else {
                imageContainer[i].classList.add('square');
                imageContainer[i].parentNode.classList.add('square');
              }
            }
          }
        }
        function calculateAllImageContainerSizes(ele,newImageHeight) {
          if($(ele).length){
            let imageContainer = document.querySelectorAll(ele);
            for (let i = 0; i < imageContainer.length; i++) {
              //Get image original dimensions
              let imageWidth = parseInt(imageContainer[i].dataset.imageWidth,10);
              let imageHeight = parseInt(imageContainer[i].dataset.imageHeight,10);
              //New dimensions
              let newImageWidth = "" + (imageWidth/imageHeight)*newImageHeight;
              //Set new width (as a scale) of original image width
              imageContainer[i].style.width = newImageWidth + "px";
              imageContainer[i].style.height = newImageHeight + "px";
              if(imageWidth < imageHeight) {
                imageContainer[i].classList.add('portrait');
                imageContainer[i].parentNode.classList.add('portrait');
              }
              else if(imageWidth > imageHeight) {
                imageContainer[i].classList.add('landscape');
                imageContainer[i].parentNode.classList.add('landscape');
              }
              else {
                imageContainer[i].classList.add('square');
                imageContainer[i].parentNode.classList.add('square');
              }
            }
          }
        }
