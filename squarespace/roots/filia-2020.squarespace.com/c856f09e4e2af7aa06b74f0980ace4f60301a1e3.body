              autoplay: true,
              autoplaySpeed: 3000,
              centerMode: false,
              adaptiveHeight: false,
              fade: true,
              cssEase: 'ease'
            });
          }
        }
        function sidebarOverlayNav() {
          if ($('a[href="#subscribe"]').length) {
            var $body = $('body');
            $('a[href="#subscribe"]').click(function(e){
              console.log('open sidebar')
              e.preventDefault();
              $body.removeClass('sidebar-overlay--closed').addClass('sidebar-overlay--opening');
              setTimeout(function(){
                $body.removeClass('sidebar-overlay--opening').addClass('sidebar-overlay--open');
              },1000);
            });
            function closeSidebarOverlayNav() {
              if (!$body.hasClass('sidebar-overlay--opening') && !$body.hasClass('sidebar-overlay--closing')) {
                $body.addClass('sidebar-overlay--closing').removeClass('sidebar-overlay--open');
                setTimeout(function(){
                  $body.removeClass('sidebar-overlay--closing').addClass('sidebar-overlay--closed');
                },1000);
              }
            }
            //Check to see if click is outside of .Sidebar-overlay-nav but within #sidebar-overlay
            $(document).click(function(e) {
              console.log($(e.target).closest('.Sidebar-overlay-nav').length)
              if(!$(e.target).closest('.Sidebar-overlay-nav').length && $(e.target).closest('#sidebar-overlay').length && $(e.target).attr('href') !== '#subscribe' && !$('body[class*="sqs-edit-mode"]').length) {
                closeSidebarOverlayNav()
              }
            });
            $('#sidebar-overlay--close').click(function(){
              closeSidebarOverlayNav()
            });
          }
        }
        function lazyLoadImages() {
          
          "use strict";

          // When image is loaded, remove the .not-ready animation class
          var removeImgLoadingClass = function removeImgLoadingClass(e) {
            //console.log(e.target)
            e.target.parentNode.classList.remove('not-ready');

            //Remove the background colour from items after image is loaded to stop 1px slithers of colour showing through
            var backgroundToRemove = e.target.parentNode.parentNode;

            if (backgroundToRemove.className.match(new RegExp('(\\s|^)remove-background-colour-after-image-loaded(\\s|$)'))) {
              setTimeout(function(){
                backgroundToRemove.removeAttribute('style');
              },800)
            }
          };
          //LAZY LOAD IMAGES
          var images = document.querySelectorAll('[data-src][data-load="false"]:not(.sqs-gallery-design-strip-slide)');
          var config = {
            rootMargin: '0px 0px 50px 0px',
            threshold: 0
          };
          var loaded = 0;
          var observer = new IntersectionObserver(function (entries, self) {
            entries.forEach(function (entry) {
              //console.log('entry',entry)
              if (entry.isIntersecting) {
                //console.log('Image' + entry.target + ' is in the viewport!');
                var sync = function sync() {
                  //Monitor the load of the image. Note this does not work for Parallax images as they are moved from section.Intro to div.Parallax-item
                  //If using a systemCollectionBlog, images will not lazy load, they will be loaded already. Check for src and do not bother listening for the load event
                  if (entry.target.hasAttribute('src')) {
                    //console.log('Image already loaded')
                    //Image already loaded
                    removeImgLoadingClass(entry);
                  }
                  else {
                    //Listen for load
                    entry.target.addEventListener('load', removeImgLoadingClass);
                  }

                  //Get the ImageLoader mode from the parentNode classList
                  var imageParent = entry.target.parentNode;
                  var ImageLoaderMode;

                  //Assume if the parent does not have the class content-fit that it should be content-fill
                  if (imageParent.className.match(new RegExp('(\\s|^)content-fit(\\s|$)'))) {
                    ImageLoaderMode = 'fit';
                  } else {
                    ImageLoaderMode = 'fill';
                  }

                  //Load the images
                  //Ignore these images
                  if(entry.target.id !== '5eb3f0ba39d69d48c414ad50' || entry.target.id !== '5ece66d2d631254570bc902c') {
                    //Load these images
                    ImageLoader.load(entry.target, {
                      load: true,
                      mode: ImageLoaderMode
                    });
                  }

                  //Show Parallax images without listening for image load
                  if(imageParent.className.match(new RegExp('(\\s|^)Intro-image(\\s|$)')) && imageParent.parentNode.className.match(new RegExp('(\\s|^)Parallax-item(\\s|$)'))){
                    //Ignore homepage parallax header image
                    if (entry.target.id !== '5eb3f0ba39d69d48c414ad50') {
                      imageParent.classList.remove('not-ready')
                    }
                  }
                };

                var destroy = function destroy() {
                  //Stop monitoring the load of the image
                  entry.target.removeEventListener('load', removeImgLoadingClass);
                };

                sync();
                return {
                  sync: sync,
                  destroy: destroy
                }; // Stop watching and load the image

                self.unobserve(entry.target);
              }
            });
          }, config);
          images.forEach(function (image) {
            observer.observe(image);
          });
        }
        return {
          enableDoubleClickToEditAnEmptySqsLayout: enableDoubleClickToEditAnEmptySqsLayout,
          throttledFunctions: throttledFunctions,
