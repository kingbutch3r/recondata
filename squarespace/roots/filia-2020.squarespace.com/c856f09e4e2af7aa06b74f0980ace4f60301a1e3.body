            $('.sqs-block-quote').each(function(index) {
              console.log('index',index)
              $(this).appendTo($('.slick-quote-blocks')).wrap('<div class="image-slide" />');
            });
            let slickEle = $('.slick-quote-blocks');
            slickEle.wrap('<div class="slick-wrapper" />');
            slickEle.slick({
              dots: true,
              infinite: true,
              speed: 800,
              slidesToShow: 1,
