        function blogFilter() {

          if($('#collection-5fc11498e18c5c478e22102a').length) {

            //Sort the tag filter alphabetically
            $(".blog-filter--tags li").sort(function(a, b) {
                var dataFilterA = $(a).attr('data-filter'), dataFilterB = $(b).attr('data-filter');
                return dataFilterA < dataFilterB ? -1 : dataFilterA > dataFilterB ? 1 : 0;
            }).appendTo('.blog-filter--tags ul');

          
            function checkboxIsChecked($jQueryObj) {
              if($jQueryObj.prop('checked') == true || $jQueryObj.is(':checked')) {
                return true;
              }
              return false;
            }

            var downloading = false;
            var allItems = {'items':[]};

            function loop(_offset,_filter,_skip) {

              //Correct layout one more time after excerpt height has been accounted for
              $grid.masonry('layout')
              
              console.log('looping')
              downloading = true;
              $('body').addClass('downloading');

              var offset = (_offset !== null) ? '&offset='+_offset : ''; //Page offset long-number
              var filter = (_filter !== null) ? _filter : ''; //Query string of SQS collection filter e.g. &category=Podcast
              var skip = (_skip === true) ? _skip : false; //Skip to next page?
              var fetchUrl = window.location.origin + window.location.pathname+'?format=json'+offset+filter;
              var data = '';

              console.log('fetchUrl',fetchUrl)

              fetch(fetchUrl)
              .then((resp) => resp.json())
              .then(function(data){

                console.log('data',data)
                var items = '';
                var nextPage = '';

                //Check if returned data has any items
                if (data.hasOwnProperty('items')) {
                  
                  $('.message-no-items').addClass('hidden')
                  items = data.items;

                  //Get last item ID
                  //var lastItemId = items[items.length - 1]['id'];

                  if(data.hasOwnProperty('pagination')) {
                    if (data.pagination.hasOwnProperty('nextPage')) {
                      nextPage = true;
                      /*if(skip) {
                        var lastItem = $('.masonry-item').last().attr('id');
                        lastItemId = lastItem.split('post-').join('')
                        console.log(lastItemId)
                      }*/
                    }
                  }
                  else {
                    nextPage = false;
                  }
                  console.log('nextPage',nextPage)
                  
                  //Render this data or skip to next page of data
                  if(!skip) {

                    existingItems = allItems.items;
                    console.log(existingItems)

                    existingItems.push(...items);
                    console.log(existingItems)

                    newItems = existingItems;
                    console.log(newItems)

                    allItems.items = newItems;
                    console.log(allItems)

                    var itemsToRender = {'items':items};
                    console.log(itemsToRender)

                    //Requires custom version of json-template.js (use minified as it is a large file)
                    //Include modules we need: node, squarespace-json-template, squarespace-data for comfort AJAX (you may use Y.io offcourse)
                    Y.use(['node', 'squarespace-json-template'], function (Y) {
                      
                      //Looping through all queries on page
                      Y.all('[data-template]').each(function (query) {
                        
                        //Check template exists on page
                        if (Y.one(query.getData('template'))) {
                          
                          //Replace our custom character to open curly brace
                          var template = Y.one(query.getData('template')).getHTML().replace(/%/g, '{');

                          //Compile template with received data
                          var compiled = Y.JSONTemplate.evaluateJsonTemplate(template, itemsToRender);
                          
                          //Insert compiled template and remove our empty query div from  document
                          //query.insert(compiled, 'before').remove();
                          //query.insert(compiled);
                          masonryLayout(compiled,items.length)
                        }
                      })
                    });
                  }
                  //Watch the last element on the page so that we can pause the loop and start it lazily when we scroll to it
                  var elementsToWatch = document.querySelectorAll('#infinite-scroll-trigger');
                  var config = {
                    rootMargin: '0px',
                    threshold: 0
                  };
                  var observer = new IntersectionObserver(function (entries, self) {
                    entries.forEach(entry => {
                      if (entry.isIntersecting) {
                        self.unobserve(entry.target);
                        console.log('Last item is in the viewport!');
                        console.log('nextPageObserver',nextPage)
                        if (nextPage === true && downloading === false) {
                          $('.masonry-layout').addClass('loading-dark')
                          console.log('_filter',_filter)
                          loop(data.pagination.nextPageOffset,_filter)
                        }
                        else {
