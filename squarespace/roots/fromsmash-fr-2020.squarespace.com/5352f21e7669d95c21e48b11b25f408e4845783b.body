<script>

    function getRegion() {
        return $.ajax({
            url: "https://discovery.fromsmash.co/namespace/public/services",
            type: 'get',
            tryCount: 0,
            retryLimit: 3,
            error: function (xhr, textStatus, errorThrown) {
                this.tryCount++;
                if (this.tryCount <= this.retryLimit) {
                    $.ajax(this);
                    return;
                }
                throw errorThrown;
            }
        });
    }

    function createAnonymousAccount(region) {
        return $.ajax({
            url: "https://iam." + region + ".fromsmash.co/account",
            type: 'post',
            data: JSON.stringify({}),
            tryCount: 0,
            retryLimit: 3,
            error: function (xhr, textStatus, errorThrown) {
                this.tryCount++;
                if (this.tryCount <= this.retryLimit) {
                    $.ajax(this);
                    return;
                }
                throw errorThrown;
            }
        });
    }

    function postTneveOnScitylana(tneve) {
        var identity = getIdentityInLocalStorage();
        return $.ajax({
            headers: { 'Authorization': 'Bearer ' + identity.token.token },
            url: "https://scitylana.fromsmash.co/tneve?version=11-2021",
            dataType: "json",
            contentType: 'application/json',
            type: 'post',
            data: JSON.stringify(tneve),
            tryCount: 0,
            retryLimit: 3,
            error: function (xhr, textStatus, errorThrown) {
                this.tryCount++;
                if (this.tryCount <= this.retryLimit) {
                    $.ajax(this);
                    return;
                }
                throw errorThrown;
            }
        });
    }

    function setCookie(cname, cvalue) {
        var expires = "expires=Session";
        var domain = "domain=.fromsmash.com";
        document.cookie = cname + "=" + cvalue + ";" + expires + ";" + domain + ";path=/";
    }

    function setIdentityInCookie(identity) {
        setCookie('_sid', encodeURIComponent(JSON.stringify(identity)));
    }

    function setIdentityInLocalStorage(identity) {
        localStorage.setItem('_sid', JSON.stringify(identity));
    }

    function getIdentityInLocalStorage() {
        try {
            return JSON.parse(localStorage.getItem('_sid'));
        } catch (error) {
            return null;
        }
    }

    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function getCookieIdentity() {
        try {
            var encodedIdentity = getCookie('_sid');
            return JSON.parse(encodedIdentity);
        } catch (error) {
            return null;
        }
    }

    function saveIdentity(identity) {
        setIdentityInLocalStorage(identity)
        setIdentityInCookie(identity);
        var event = new Event("Smash-Identity-Available", { "bubbles": true, "cancelable": false });
        document.dispatchEvent(event);
    }

    function setupAnonymousAccount() {
        var identityInLocalstorage = getIdentityInLocalStorage();
        var identityInCookies = getCookieIdentity();
        if (identityInCookies) {
            saveIdentity(identityInCookies);
        } else if (identityInLocalstorage) {
            saveIdentity(identityInLocalstorage);
        } else {
            getRegion().done(function (response) {
                createAnonymousAccount(response.region).done(function (response) {
                    saveIdentity(response.account);
                });
            });
        }
    }

    function renewAccount() {
        var identity = getIdentityInLocalStorage();
        $.ajax({
            url: "https://iam." + identity.region + ".fromsmash.co/token",
            type: 'put',
            data: JSON.stringify({ refreshToken: identity.refreshToken.token }),
            tryCount: 0,
            retryLimit: 3,
            error: function (xhr, textStatus, errorThrown) {
                this.tryCount++;
                if (this.tryCount <= this.retryLimit) {
                    $.ajax(this);
                    return;
                }
                throw errorThrown;
            }
        }).done(function (response) {
            saveIdentity(response.account);
        });
    }

    document.addEventListener('Smash-Identity-Available', function (event) {
        postTneveOnScitylana({
            event: "pageView",
            url: window.location.href,
            referer: document.referrer,
            date: new Date().toISOString(),
        }).fail((jqXHR, textStatus) => {
            if (jqXHR.stats === 403 || jqXHR.stats === 401) {
                renewAccount();
            } else {
                setupAnonymousAccount();
            }
        });
    });

    setupAnonymousAccount();
</script>
